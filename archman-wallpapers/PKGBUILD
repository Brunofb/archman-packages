pkgname=archman-wallpapers
pkgver=0.1
pkgrel=2
pkgdesc='Archman GNU/Linux wallpapers'
arch=('any')
url='http://archman.org'
license=('GPL3')
depends=('')
conflicts=('')
source=("http://archman.org/source/archman-wallpapers.tar.gz")
sha1sums=('41d7507ce4eae38b552f56a73dc28731d939720a')


package() {
  install -d -m 0755 $pkgdir/usr/share/Wallpaper/
  cp -r * $pkgdir/usr/share/Wallpaper/
  rm $pkgdir/usr/share/Wallpaper/archman-wallpapers.tar.gz
}